      <media:content url="https://imagenes.elpais.com/resizer/v2/DN7ZTRYMSRAVHP5JOA7THTQWHQ.jpg?auth=7a8f5694fe7dbe3c734639b964ad3bd6d83496d70040eb71bbf0483815454189" type="image/jpeg" medium="image">

      </media:content>
      <media:group>
        <media:content url="https://imagenes.elpais.com/resizer/v2/G4GAGAQSMFGSDGUZN4L2GJ6XCI.jpg?auth=399a4a23e694351f4bb5800e11923075e3f6229b95598c7e994dff70f1122637" type="image/jpeg" medium="image">

      <media:content url="https://imagenes.elpais.com/resizer/v2/CB7NW6SVZ5H2HGAMVZ6MSJQW5Y.jpg?auth=bc91afaef3ee2996409b563b79dddf3b6005f2de04df4767c18c4a3eedc05bc5" type="image/jpeg" medium="image">

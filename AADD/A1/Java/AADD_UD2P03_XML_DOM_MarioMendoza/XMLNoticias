      <media:content url="https://imagenes.elpais.com/resizer/v2/Z43JQKE52RA3NOMJGXKO7OB3QM.jpg?auth=dca5552462f9ece5b08c615e5977e5f249a730aed0b1e74f129fae1c9d0fdd80" type="image/jpeg" medium="image">

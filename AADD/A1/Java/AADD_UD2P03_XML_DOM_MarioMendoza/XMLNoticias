      <media:content url="https://imagenes.elpais.com/resizer/v2/CCYPKIWPWZGNLDHHZ5SUC3CWV4.jpg?auth=4cae29fc22a3bce1cab42ef908cc6c526775543f0cdab8732717b38b16b71aaa" type="image/jpeg" medium="image">

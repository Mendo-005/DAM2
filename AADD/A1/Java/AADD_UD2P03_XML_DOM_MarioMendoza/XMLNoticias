          <media:text>Vestido sin tirantes modelo Beatrix. </media:text>
          <media:description>Vestido sin tirantes modelo Beatrix. </media:description>
        </media:content>
        <media:content url="https://imagenes.elpais.com/resizer/v2/U6WRFQEZLJE6TBVFCKRWXG2CO4.jpg?auth=85b90797c5a20f661d55a4a0acebbac0a6259857ef85b8b53344bd0fbb7ad197" type="image/jpeg" medium="image">

      <media:content url="https://imagenes.elpais.com/resizer/v2/UUXV4OJFEJCV5FME6VMP7US7EY.jpg?auth=ae06865b96e7207bad0abea71b90a76ce2b87236d78b0ab8382a5598a4fd4497" type="image/jpeg" medium="image">

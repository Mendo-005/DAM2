      <media:content url="https://imagenes.elpais.com/resizer/v2/I5DOSPZBPVGBRPVSM4POWAXAAQ.JPG?auth=a590930847197f6c85fc131d49d2e027e269e8b79152a30c47a417ffb84b248e" type="image/jpeg" medium="image">

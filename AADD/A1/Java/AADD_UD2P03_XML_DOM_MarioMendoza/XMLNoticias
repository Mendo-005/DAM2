      <category>Mónica García Gómez</category>
      <category>Alemania</category>
      <media:content url="https://imagenes.elpais.com/resizer/v2/53HSPM3EEJHIVIOHAZO45PJD5I.jpg?auth=f1d5d0cf2ca634c99e774559887b459d5a11c27cac001fe9fa6e0f2f8df63367" type="image/jpeg" medium="image">

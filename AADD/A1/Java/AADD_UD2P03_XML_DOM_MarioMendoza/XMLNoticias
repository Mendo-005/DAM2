      <category>Novo Nordisk</category>
      <media:content url="https://imagenes.elpais.com/resizer/v2/TGWD27QTYFGX5FCTZCG3KVFKMM.jpg?auth=23711c364d1b8273008cd6a763688622cf77442e5e9cb105b9c7c597d5744a5a" type="image/jpeg" medium="image">

      <media:content url="https://imagenes.elpais.com/resizer/v2/5A52UECHBJB33ML6E7RRWI2A44.jpg?auth=1d3108cda4f9e56db36d8c677184f09dd183fa49e2ec0546af0447ae2237f74a" type="image/jpeg" medium="image">
